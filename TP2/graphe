library(ggplot2)

perf <- read.table("perf_commun_sans_insertion.dat", header = TRUE)

ggplot(perf,aes(x=taille, y=mem)) + geom_point() + geom_smooth() + ggtitle("Consommation mémoire tri radix sur tableau aléatoire")

ggplot(perf,aes(x=taille, y=temps, group=nom_tri, colour=as.character(nom_tri))) + geom_point() + geom_smooth() + ggtitle("Temps d'execution par type de tableau et par tri hors insertion") + facet_grid(. ~ type_tab)

ggsave("graphe_commun_sans_insertion_temps.png")

